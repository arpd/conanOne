{
	"name": "conanone",
	"displayName": "conanOne",
	"description": "Conan 1.5x command",
	"version": "0.1.0",
	"publisher": "svbtl",
	"repository": "https://github.com/arpd/conanOne",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.conanSetUser",
				"title": "conan/set user"
			},
			{
				"command": "extension.conanSetChannel",
				"title": "conan/set channel"
			},
			{
				"command": "extension.conanCreate",
				"title": "conan/create"
			},
			{
				"command": "extension.conanSearch",
				"title": "conan/search"
			},
			{
				"command": "extension.conanInfo",
				"title": "conan/info"
			},
			{
				"command": "extension.conanSelectProject",
				"title": "conan/select project"
			},
			{
				"command": "extension.conanOpenBuildDirTerm",
				"title": "conan/open project build directory"
			},
			{
				"command": "extension.conanOpenPackageDirTerm",
				"title": "conan/open project package directory"
			}
		],
		"configuration": {
			"title": "ConanOne",
			"properties": {
				"conanOne.general.conanUserHome": {
					"type": "string",
					"default": null,
					"markdownDescription": "Path to the conan cache to use\n\n- **Overrides** `CONAN_USER_HOME` in your environment if set.\n\n- **Overridden** by using `conan select cache`.",
					"order": 0
				},
				"conanOne.general.channel": {
					"type": "string",
					"default": null,
					"markdownDescription": "Conan **channel** to tag builds with\n\n_e.g._ `conan create . USER/channel`",
					"order": 1
				},
				"conanOne.general.user": {
					"type": "string",
					"default": null,
					"markdownDescription": "Conan **user** to tag builds with\n\n_e.g._ `conan create . user/CHANNEL`",
					"order": 2
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^10.0.1",
		"@types/node": "^18",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"@vscode/test-electron": "^2.3.9",
		"eslint": "^8.26.0",
		"glob": "^7.1.4",
		"mocha": "^10.2.0",
		"source-map-support": "^0.5.12",
		"typescript": "^5.4.2"
	},
	"dependencies": {
		"@vscode/vsce": "^2.26.1",
		"discord.js": "^14.15.2"
	}
}